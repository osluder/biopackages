%{_datadir}/microarray/HG-Focus/*.CEL
%{_datadir}/microarray/HG-U133A/*.CEL
%{_datadir}/microarray/HG-U133A_2/*.CEL
%{_datadir}/microarray/HG-U133B/*.CEL
